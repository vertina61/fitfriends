{
  "name": "fitfriends",
  "version": "1.0.0",
  "description": "FitFriends — это онлайн площадка для поиска тренировок и их создания.",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.rest.js",
    "start:dev": "npm run ts ./src/main.rest.ts | pino-pretty --colorize --translateTime SYS:standard",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "mock:server": "json-server ./mocks/mock-server-data.json --port 3123",
    "cli": "ts-node ./src/main.cli.ts"
  },
  "devDependencies": {
    "@types/convict": "6.1.6",
    "@types/convict-format-with-validator": "6.0.5",
    "@types/express": "4.17.21",
    "@types/node": "18.17.17",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "json-server": "1.0.0-alpha.23",
    "nodemon": "3.0.3",
    "pino-pretty": "10.3.1",
    "rimraf": "5.0.1",
    "ts-node": "^10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
    "type": "module",
    "engines": {
    "node": "20.11.1",
    "npm": ">=10.2.5"
  },
  "dependencies": {
    "@nestjs/swagger": "7.3.0",
    "@typegoose/typegoose": "12.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "config": "^3.3.11",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "dayjs": "1.11.10",
    "dotenv": "16.4.4",
    "editor": "^1.0.0",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "got": "14.2.0",
    "http-status-codes": "2.3.0",
    "inversify": "6.0.2",
    "jose": "5.2.2",
    "mongoose": "8.1.2",
    "pino": "8.18.0",
    "reflect-metadata": "0.2.1"
  }
}
